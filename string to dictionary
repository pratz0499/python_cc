class connected:
    def cs(self):
        self.server=input()
        self.dbname=input()
        self.usr=input()
        self.passs=input()


    def show(self):
        print("server= %s,dbname= %s,usr= %s,passs= %s" %(self.server,self.dbname,self.usr,self.passs))
        return("server= %s,dbname= %s,usr= %s,passs= %s" %(self.server,self.dbname,self.usr,self.passs))



    def showtodic(show):
               dictionary={}
               for i in show.split(","):
                   key,vlaue=i.split("=")
                   dictionary[key]=value
                   print(dictionary)
          
          


           
    
x=connected()
x.cs()
x.show()
