def ipt():
        n=int(input())
#import pdb; pdb.set_trace()
        i=0
        j=0
        l=[]
        for x in range(1,10):
                p=(x**n)
                i=i+1
                l.insert(i,p)

        return l


def pattern(l):
        length=len(l)
        for j in range(length-1):
                dif=(l[j+1])-(l[j])
                l[j]=dif
        #l.insert(j,dif)



        l.pop(-1)
        pattern (l)
        print(l)  

z=ipt()
a=pattern(z)

